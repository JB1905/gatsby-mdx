    "@babel/preset-react": "^7.0.0",
    "@babel/standalone": "^7.2.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "jest": "^23.6.0",
    "jest-enzyme": "^7.0.1",
    "prettier": "^1.15.3",
    "react": "^16.6.3",
